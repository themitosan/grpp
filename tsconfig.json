{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "rootDir": "./src",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./App",
        "removeComments": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
    },
    "include": ["./src/**/*"]
}
