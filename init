# Make logo executable and display it
chmod +x src/shell/logo
./src/shell/logo

# Make scripts executable
echo -e "Making scripts as executable"
chmod +x import
chmod +x update
chmod +x src/shell/process_update.sh

# Execute node init script
node src/js/init.js

# End process
echo 
echo -e "=== Process Complete ==="
echo -e "Press \033[1;32m[ENTER]\033[0m to exit"
read -p ""
clear